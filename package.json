{
  "name": "linkify-plus-plus-core",
  "version": "0.2.0",
  "description": "A JavaScript library for linkification stuff",
  "keywords": [
    "linkify"
  ],
  "homepage": "https://github.com/eight04/linkify-plus-plus-core",
  "bugs": {
    "url": "https://github.com/eight04/linkify-plus-plus-core/issues",
